{"ast":null,"code":"import { takeLatest, take } from 'redux-saga/effects';\nimport * as types from '../actions/userActions';\nimport * as typess from '../actions/flightActions';\nimport * as typees from '../actions/offersActions';\nimport * as typpes from '../actions/packagesAction';\nimport { getTokenSaga, loginSaga, logOutSaga, setPasswordSaga, updatePasswordSaga, verifyEmailSaga, verifyOtpSaga } from '../sagas/userSaga';\nimport { bookingCreation, flightSaga, flightValidation, getSavedBooking, pnrCreation } from './flightSaga';\nimport { createOfferSaga, deleteOfferSaga, offersSaga, updateOfferSaga } from './offersSaga';\nimport { createPackageSaga, deletePackageSaga, packageSaga, updatePackageSaga } from './packagesSaga';\nexport default function* watchUserAuthentication() {\n  yield takeLatest(types.GET_TOKEN, getTokenSaga);\n  yield takeLatest(types.USER_LOGIN_REQUEST, loginSaga);\n  yield takeLatest(typess.REQUEST_FLIGHT_DATA, flightSaga);\n  yield takeLatest(typess.FLIGHT_REVALIDATE, flightValidation);\n  yield takeLatest(typess.CREATE_PNR, pnrCreation);\n  yield takeLatest(typess.SAVE_BOOKING, bookingCreation);\n  yield takeLatest(typess.GET_SAVE_BOOKING, getSavedBooking);\n  yield takeLatest(types.VERIFY_EMAIL, verifyEmailSaga);\n  yield takeLatest(types.CONFIRM_OTP, verifyOtpSaga);\n  yield takeLatest(types.SET_PASSWORD, setPasswordSaga);\n  yield takeLatest(types.UPDATE_PASSWORD, updatePasswordSaga);\n  yield takeLatest(typees.READ_OFFER, offersSaga);\n  yield takeLatest(typpes.READ_PACKAGES, packageSaga);\n  yield takeLatest(typpes.CREATE_PACKAGES, createPackageSaga);\n  yield takeLatest(typpes.DELETE_PACKAGES, deletePackageSaga);\n  yield takeLatest(typpes.UPDATE_PACKAGES, updatePackageSaga);\n  yield takeLatest(typees.CREATE_OFFER, createOfferSaga);\n  yield takeLatest(typees.DELETE_OFFER, deleteOfferSaga);\n  yield takeLatest(typees.UPDATE_OFFER, updateOfferSaga);\n  yield take(types.USER_LOGOUT, logOutSaga);\n  console.log('Hello coder from watchers!!');\n}","map":{"version":3,"sources":["D:/flight380-Mar12-master/redux/sagas/watchers.js"],"names":["takeLatest","take","types","typess","typees","typpes","getTokenSaga","loginSaga","logOutSaga","setPasswordSaga","updatePasswordSaga","verifyEmailSaga","verifyOtpSaga","bookingCreation","flightSaga","flightValidation","getSavedBooking","pnrCreation","createOfferSaga","deleteOfferSaga","offersSaga","updateOfferSaga","createPackageSaga","deletePackageSaga","packageSaga","updatePackageSaga","watchUserAuthentication","GET_TOKEN","USER_LOGIN_REQUEST","REQUEST_FLIGHT_DATA","FLIGHT_REVALIDATE","CREATE_PNR","SAVE_BOOKING","GET_SAVE_BOOKING","VERIFY_EMAIL","CONFIRM_OTP","SET_PASSWORD","UPDATE_PASSWORD","READ_OFFER","READ_PACKAGES","CREATE_PACKAGES","DELETE_PACKAGES","UPDATE_PACKAGES","CREATE_OFFER","DELETE_OFFER","UPDATE_OFFER","USER_LOGOUT","console","log"],"mappings":"AAAA,SAASA,UAAT,EAAqBC,IAArB,QAAiC,oBAAjC;AACA,OAAO,KAAKC,KAAZ,MAAuB,wBAAvB;AACA,OAAO,KAAKC,MAAZ,MAAwB,0BAAxB;AACA,OAAO,KAAKC,MAAZ,MAAwB,0BAAxB;AACA,OAAO,KAAKC,MAAZ,MAAwB,2BAAxB;AACA,SACEC,YADF,EAEEC,SAFF,EAGEC,UAHF,EAIEC,eAJF,EAKEC,kBALF,EAMEC,eANF,EAOEC,aAPF,QAQO,mBARP;AASA,SACEC,eADF,EAEEC,UAFF,EAGEC,gBAHF,EAIEC,eAJF,EAKEC,WALF,QAMO,cANP;AAOA,SACEC,eADF,EAEEC,eAFF,EAGEC,UAHF,EAIEC,eAJF,QAKO,cALP;AAMA,SACEC,iBADF,EAEEC,iBAFF,EAGEC,WAHF,EAIEC,iBAJF,QAKO,gBALP;AAOA,eAAe,UAAUC,uBAAV,GAAoC;AACjD,QAAM1B,UAAU,CAACE,KAAK,CAACyB,SAAP,EAAkBrB,YAAlB,CAAhB;AACA,QAAMN,UAAU,CAACE,KAAK,CAAC0B,kBAAP,EAA2BrB,SAA3B,CAAhB;AACA,QAAMP,UAAU,CAACG,MAAM,CAAC0B,mBAAR,EAA6Bf,UAA7B,CAAhB;AACA,QAAMd,UAAU,CAACG,MAAM,CAAC2B,iBAAR,EAA2Bf,gBAA3B,CAAhB;AACA,QAAMf,UAAU,CAACG,MAAM,CAAC4B,UAAR,EAAoBd,WAApB,CAAhB;AACA,QAAMjB,UAAU,CAACG,MAAM,CAAC6B,YAAR,EAAsBnB,eAAtB,CAAhB;AACA,QAAMb,UAAU,CAACG,MAAM,CAAC8B,gBAAR,EAA0BjB,eAA1B,CAAhB;AACA,QAAMhB,UAAU,CAACE,KAAK,CAACgC,YAAP,EAAqBvB,eAArB,CAAhB;AACA,QAAMX,UAAU,CAACE,KAAK,CAACiC,WAAP,EAAoBvB,aAApB,CAAhB;AACA,QAAMZ,UAAU,CAACE,KAAK,CAACkC,YAAP,EAAqB3B,eAArB,CAAhB;AACA,QAAMT,UAAU,CAACE,KAAK,CAACmC,eAA